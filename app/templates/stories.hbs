<div class="container full-page-container">
  <h1>Recent Stories</h1>
  {{log model}}
  <ul class="story-list">
    {{#each model as | story | }}



    <li>
      <div class="story-container well">
      <h3 class="story-title">{{story.title}}</h3>
      <p class="story-summary">{{story.summary}}</p>
      </div>
    </li>
    {{/each}}
  </ul>
  {{outlet}}
</div>
